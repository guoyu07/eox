<% include head %>
<h2><%= post.name %></h1>
<div><%= post.content %></div>
<ul>
<% for ( var i = 0, len = comments.length, comment; i < len; i++ ) { %>
<% comment = comments[ i ]; %>
<li>
<p><a href="/user/<%= comment.userId %>"><%= comment.userName %></a></p>
<div><%= comment.content %></div>
</li>
<% } %>
</ul>
<% include foot %>
